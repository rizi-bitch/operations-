num1=int(input("enter the first number:"))
num2=int(input("enter the second number:"))
operation=input("enter the operations:")
if operation=="+":
    print(num1+num2)
elif operation=="-":
    print(num1-num2)
elif operation=="/":
    print(num1/num2)
elif operation=="*":
    print(num1*num2)
else:
    print("error")
